{
  "name": "itinerarymanagement.client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build --configuration production && npm run sentry:sourcemaps",
    "build:ci": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org vsb --project javascript-angular dist && sentry-cli sourcemaps upload --org vsb --project javascript-angular dist",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.3",
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^20.3.3",
    "@angular/material": "^20.2.5",
    "@angular/material-moment-adapter": "^20.2.5",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@auth0/angular-jwt": "^5.2.0",
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@sentry/angular": "^10.17.0",
    "@sentry/cli": "^2.56.0",
    "@types/node-geocoder": "^4.2.6",
    "@wabson/mapbox-gl-style-switcher": "^0.0.3",
    "jest-editor-support": "*",
    "jwt-decode": "^4.0.0",
    "mapbox-gl": "^3.15.0",
    "mapbox-gl-style-switcher": "^1.0.11",
    "ngx-lightbox": "^3.0.0",
    "ngx-toastr": "^19.1.0",
    "run-script-os": "*",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.0",
    "weathericons": "^2.1.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.4",
    "@angular/cli": "^20.3.4",
    "@angular/compiler-cli": "^20.3.3",
    "@types/jasmine": "^5.1.9",
    "@types/jwt-decode": "^3.1.0",
    "@types/mapbox__mapbox-gl-geocoder": "^5.0.0",
    "@types/mapbox-gl": "^3.4.1",
    "angular-eslint": "20.3.0",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.1.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  }
}
